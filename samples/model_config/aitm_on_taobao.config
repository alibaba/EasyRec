  optimizer_config {
    adam_optimizer {
      learning_rate {
        constant_learning_rate {
          learning_rate: 0.0001
        }
      }
    }
    use_moving_average: false
  }
  num_steps: 500
  sync_replicas: true
  save_checkpoints_steps: 100
  log_step_count_steps: 100
}
